<div class="containter">

<h1>All Products</h1>
<div class="allProducts">
{{#each allProducts}}
<div id="productImg">
    <img src="{{images}}">
</div>
<div id="productInfo">
    <ul>
        <li>Name: {{productName}}</li>
        <li>Price: {{price}}</li>
     
    </ul>
    <a href="/products/{{_id}}">Check this out</a>
</div>
{{/each}}
</div>
</div>
